<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.silkboardapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SO1" />
        <activity android:name=".SO2" />
        <activity android:name=".SO3" />
        <activity android:name=".SO4" />
        <activity android:name=".SO5" />
        <activity android:name=".SO6" />
        <activity android:name=".SO7" />
        <activity android:name=".SO0"></activity>
    </application>

</manifest>